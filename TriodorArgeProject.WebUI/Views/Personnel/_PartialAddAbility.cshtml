@model TriodorArgeProject.Entities.EntityClasses.PersonnelAbility
<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <form action="/Ability/AddAbility/" enctype="multipart/form-data" method="post">
                <div class="form-body">
                    <div class="row p-t-20">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label for="registerno">Ability :</label>
                                <input type="text" class="form-control" id="ability" name="ability">
                            </div>
                        </div>
                       
                    </div>
                    <button type="submit" class="btn btn-info">Add</button>
                </div>
            </form>
        </div>
    </div>
</div>
