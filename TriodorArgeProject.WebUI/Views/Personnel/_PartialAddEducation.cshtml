@model TriodorArgeProject.Entities.EntityClasses.PersonnelEducations
<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <form action="/Education/AddEducation/" enctype="multipart/form-data" method="post">
                <div class="form-body">
                    <div class="row p-t-20">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label for="registerno">Education Description :</label>
                                <input type="text" class="form-control" id="education" name="education">
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="form-group">
                                <label for="registerno">Start Date :</label>
                                <input type="date" class="form-control" id="startdate" name="startdate">
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="form-group">
                                <label for="registerno">End Date :</label>
                                <input type="date" class="form-control" id="enddate" name="enddate">
                            </div>
                        </div>
                    </div>
                    <button type="submit" class="btn btn-info">Add</button>
                </div>
            </form>
        </div>
    </div>
</div>